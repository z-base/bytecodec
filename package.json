{
  "name": "bytecodec",
  "version": "1.4.0",
  "description": "Zero-dependency byte toolkit for base64url, UTF-8 strings, JSON, CSP-ready nonces, and timing-safe equality in browser and Node runtimes.",
  "keywords": [
    "base64url",
    "base64",
    "Uint8Array",
    "bytes",
    "encoding",
    "decoding",
    "browser",
    "node",
    "utf8",
    "json",
    "nonce",
    "csp",
    "equals",
    "timing-safe"
  ],
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jortsupetterson/bytecodec.git"
  },
  "bugs": {
    "url": "https://github.com/jortsupetterson/bytecodec/issues"
  },
  "homepage": "https://github.com/jortsupetterson/bytecodec#readme",
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "node test.js && node benchmark/bench.js",
    "bench": "node benchmark/bench.js"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  }
}
